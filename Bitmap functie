int x_lo_ = 10;
int y_lo_ = 100;
int nr_ = 5;
void bitmap (int nr, int x_lo, int y_lo);

int main(void)
{
	SystemInit(); // System speed to 168MHz

	UB_VGA_Screen_Init(); // Init VGA-Screen

    UB_VGA_FillScreen(VGA_COL_WHITE);

    bitmap (nr_, x_lo_, y_lo_);

while(1)
  {
  }
}


