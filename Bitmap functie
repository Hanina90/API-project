     {0x03,0x03,0x03,0x03,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x03,0x03,0x03,0x03}};





	switch (nr){
	case 1: for(int i=0; i<20; i++)
			{
				for(int j=0; j<20; j++)
				{
					color = pijl_boven[i][j];
					if (color == 0x01) UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 0);
					else UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 255);
				}
			}
    break;
	case 2: for(int i=0; i<20; i++)
			{
				for(int j=0; j<20; j++)
				{
					color = pijl_onder[i][j];
					if (color == 0x01) UB_VGA_SetPixel((i+x_lo), (j+y_lo) ,0);
					else UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 255);
				}
			}
    break;
	case 3: for(int i=0; i<20; i++)
			{
				for(int j=0; j<20; j++)
				{
					color = pijl_links[i][j];
					if (color == 0x01) UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 0);
					else UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 255);
				}
			}
	break;
	case 4: for(int i=0; i<20; i++)
			{
				for(int j=0; j<20; j++)
				{
					color = pijl_rechts[i][j];
					if (color == 0x01) UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 0);
					else UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 255);
				}
			}
	break;
	case 5: for(int i=0; i<20; i++)
			{
				for(int j=0; j<20; j++)
				{
					color = smiley_blij[i][j];
					if (color == 0x01) UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 0);
					else if (color == 0x00) UB_VGA_SetPixel((i+x_lo), (j+y_lo) , VGA_COL_YELLOW);
					else if (color == 0x02) UB_VGA_SetPixel((i+x_lo), (j+y_lo) , VGA_COL_RED);
					else UB_VGA_SetPixel((i+x_lo), (j+y_lo) , VGA_COL_WHITE);
				}
			}
	break;
	case 6: for(int i=0; i<20; i++)
			{
				for(int j=0; j<20; j++)
				{
					color = smiley_boos[i][j];
					if (color == 0x01) UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 0);
					else if (color == 0x00) UB_VGA_SetPixel((i+x_lo), (j+y_lo) , 200);
					else UB_VGA_SetPixel((i+x_lo), (j+y_lo) , VGA_COL_WHITE);
				}
			}
	break;
	}
}
